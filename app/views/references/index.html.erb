<h1>References#index</h1>
<p>Find me in app/views/references/index.html.erb</p>

<% @references.each do |reference| %>
  <ul class="list-group">
    <li class="list-group-item d-flex justify-content-between align-items-start">
      <div class="ms-2 me-auto">
        <%= reference.name %>
      </div>
      <span class="badge rounded-pill"><%= link_to "Ver", reference_path(reference.id) %></span>
    </li>
  </ul>
<% end  %>
