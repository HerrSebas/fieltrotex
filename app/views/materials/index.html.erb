<h1>materials#index</h1>
<p>Find me in app/views/materials/index.html.erb</p>

<% @materials.each do |material| %>
  <ul class="list-group">
    <li class="list-group-item d-flex justify-content-between align-items-start">
      <div class="ms-2 me-auto">
        <%= material.name %>
      </div>
      <span class="badge rounded-pill"><%= link_to "Ver", material_path(material.id) %></span>
    </li>
  </ul>
<% end  %>
